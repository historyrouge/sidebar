  "shortcuts": [
    {
      "name": "New Chat",
      "short_name": "Chat",
      "description": "Start a new AI conversation",
      "url": "/?action=new-chat",
      "icons": [
        {
          "src": "/icons/shortcut-chat.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Study Session",
      "short_name": "Study",
      "description": "Begin a focused study session",
      "url": "/study-now",
      "icons": [
        {
          "src": "/icons/shortcut-study.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Quiz",
      "short_name": "Quiz",
      "description": "Take a quiz or assessment",
      "url": "/quiz",
      "icons": [
        {
          "src": "/icons/shortcut-quiz.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Code Analyzer",
      "short_name": "Code",
      "description": "Analyze and debug code",
      "url": "/code-analyzer",
      "icons": [
        {
          "src": "/icons/shortcut-code.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": ["text/plain", "image/*", "application/pdf"]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/handle-file",
      "accept": {
        "text/plain": [".txt", ".md"],
        "application/pdf": [".pdf"],
        "image/*": [".jpg", ".jpeg", ".png", ".gif", ".webp"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+searnai",
      "url": "/handle-protocol?url=%s"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  }
}